{
  "name": "gaguna",
  "version": "1.0.6",
  "description": "My TS utilities.",
  "author": "Hartono Chandra",
  "module": "dist/gaguna.esm.js",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=./test/custom-test-env.ts --coverage",
    "test:clear": "tsdx test --clearCache",
    "lint": "eslint src test",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "standard-version",
    "generate-docs": "typedoc --out docs src/index.ts"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.8",
    "standard-version": "^9.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts}": [
      "eslint"
    ]
  },
  "size-limit": [
    {
      "path": "dist/gaguna.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gaguna.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
